{
  "name": "eloquent-test",
  "type": "project",
  "description": "Testing some ideas around Eloquent",
  "require": {
    "illuminate/database": "~5.0",
    "illuminate/console": "~5.0",
    "illuminate/container": "~5.0",
    "illuminate/filesystem": "~5.0",
    "illuminate/events": "~5.0",
    "symfony/process": "2.6.*",
    "cviebrock/eloquent-sluggable": "dev-master",
    "cocur/slugify": "1.1.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Enl\\HelloBlog\\": "src/",
      "Enl\\HelloBlog\\Tests\\": "tests/"
    },
    "psr-0": {
      "": "src/"
    }
  }
}
